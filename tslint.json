{
  "extends": "tslint-config-airbnb",
  "linterOptions": {
    "exclude": ["graphql/index.tsx","server/graphql/types.ts"]
  },
  "rules": {
    "no-trailing-whitespace": false,
    "trailing-comma": false,
    "import-name": false,
    "align": false,
    "variable-name": false,
    "max-line-length": false,
    "function-name": false
  }
}